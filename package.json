{
  "name": "@graasp/translations",
  "version": "1.25.3",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "description": "Graasp Translations",
  "repository": {
    "type": "git",
    "url": "https://github.com/graasp/graasp-translations"
  },
  "license": "AGPL-3.0-only",
  "author": "Graasp",
  "contributors": [
    "Juan Carlos Farah",
    "Kim Lan Phan Hoang",
    "Víctor González",
    "Basile Spaenlehauer",
    "Alexandre Chau",
    "Julien Torrent",
    "Jérémy La Scala"
  ],
  "scripts": {
    "prepack": "yarn build",
    "prepare": "yarn build && yarn hooks:install",
    "prettier:check": "prettier --check {src,test}/**/*.{js,ts,tsx,json}",
    "prettier:write": "prettier --write {src,test}/**/*.{js,ts,tsx,json}",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "test:watch": "exit 0",
    "test": "exit 0",
    "lint": "eslint .",
    "type-check": "yarn tsc --noEmit",
    "check": "yarn prettier:check && yarn lint && yarn type-check",
    "pre-commit": "yarn check",
    "post-commit": "git status",
    "build": "tsc"
  },
  "peerDependencies": {
    "i18next": "^23.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "husky": "9.0.11",
    "i18next": "23.10.1",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.4.3"
  },
  "packageManager": "yarn@4.1.1"
}
