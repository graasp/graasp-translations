{
  "name": "@graasp/translations",
  "version": "1.21.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "description": "Graasp Translations",
  "repository": {
    "type": "git",
    "url": "https://github.com/graasp/graasp-translations"
  },
  "license": "AGPL-3.0-only",
  "author": "Graasp",
  "contributors": [
    "Juan Carlos Farah",
    "Kim Lan Phan Hoang",
    "Víctor González",
    "Basile Spaenlehauer",
    "Alexandre Chau",
    "Julien Torrent",
    "Jérémy La Scala"
  ],
  "dependencies": {
    "i18next": "23.7.6"
  },
  "scripts": {
    "prepack": "yarn build",
    "prepare": "yarn build && yarn hooks:install",
    "prettier:check": "prettier --check {src,test}/**/*.{js,ts,tsx,json}",
    "prettier:write": "prettier --write {src,test}/**/*.{js,ts,tsx,json}",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "test:watch": "exit 0",
    "test": "exit 0",
    "lint": "eslint .",
    "type-check": "yarn tsc --noEmit",
    "check": "yarn prettier:check && yarn lint && yarn type-check",
    "pre-commit": "yarn check",
    "post-commit": "git status",
    "build": "tsc"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.2",
    "@commitlint/config-conventional": "18.4.2",
    "@types/jest": "29.5.8",
    "@types/node": "20.9.0",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "prettier": "3.1.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "packageManager": "yarn@4.0.2"
}
