{
  "name": "@graasp/translations",
  "version": "1.15.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "description": "Graasp Translations",
  "repository": {
    "type": "git",
    "url": "https://github.com/graasp/graasp-translations"
  },
  "license": "AGPL-3.0-only",
  "author": "Graasp",
  "contributors": [
    "Juan Carlos Farah",
    "Kim Lan Phan Hoang",
    "Víctor González",
    "Basile Spaenlehauer",
    "Alexandre Chau",
    "Julien Torrent"
  ],
  "scripts": {
    "prepack": "yarn build",
    "prepare": "yarn build && yarn hooks:install",
    "prettier:check": "prettier --check {src,test}/**/*.{js,ts,tsx,json}",
    "prettier:write": "prettier --write {src,test}/**/*.{js,ts,tsx,json}",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "test:watch": "exit 0",
    "test": "exit 0",
    "lint": "eslint .",
    "pre-commit": "yarn prettier:check && yarn lint",
    "post-commit": "git status",
    "build": "tsc"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.1",
    "@commitlint/config-conventional": "17.6.1",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "packageManager": "yarn@3.2.4",
  "dependencies": {
    "i18next": "22.4.15"
  }
}
