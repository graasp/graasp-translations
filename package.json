{
  "name": "@graasp/translations",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/index.js",
  "source": "src/index.ts",
  "description": "Graasp Translations",
  "repository": {
    "type": "git",
    "url": "https://github.com/graasp/graasp-translations"
  },
  "license": "AGPL-3.0-only",
  "author": "Graasp Association",
  "scripts": {
    "prepack": "yarn build",
    "prepare": "yarn build && yarn hooks:install",
    "prettier:check": "prettier --check {src,test}/**/*.{js,ts,tsx}",
    "prettier:write": "prettier --write {src,test}/**/*.{js,ts,tsx}",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "test:watch": "exit 0",
    "test": "exit 0",
    "lint": "eslint .",
    "pre-commit": "yarn prettier:check && yarn lint",
    "post-commit": "git status",
    "build": "tsc"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.20",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "2.25.4",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "typescript": "4.1.3"
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "i18next": "21.6.11"
  }
}
