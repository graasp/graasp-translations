{
  "name": "graasp-translations",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "description": "Graasp Translations",
  "repository": {
    "type": "git",
    "url": "https://github.com/graasp/graasp-translations"
  },
  "license": "AGPL-3.0-only",
  "author": "Graasp Association",
  "scripts": {
    "prepack": "yarn build",
    "prepare": "yarn build && yarn hooks:install",
    "prettier:check": "prettier --check {src,test}/**/*.{js,ts,tsx}",
    "prettier:write": "prettier --write {src,test}/**/*.{js,ts,tsx}",
    "pre-commit": "yarn prettier:check && yarn lint",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "test:watch": "jest --watchAll",
    "test": "jest src --silent",
    "lint": "eslint .",
    "post-commit": "git status",
    "build": "tsc"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "babel-jest": "27.4.6",
    "env-cmd": "10.1.0",
    "eslint": "8.6.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.1.3"
  },
  "packageManager": "yarn@3.2.0"
}
